{
  "name": "aul2-intellisense-fork",
  "displayName": "AviUtl2 IntelliSense Fork",
  "description": "Hirokawa-beachさんのAviUtl2-IntelliSenseのフォーク版です。主にSyntax Highlight部分を変更しています。",
  "version": "0.2.0",
  "publisher": "seilor0",
  "engines": {
    "vscode": "^1.104.1"
  },
  "repository": {
  "type": "git",
  "url": "https://github.com/seilor0/AviUtl2-IntelliSense-Fork.git"
  },
  "categories": ["Languages"],
  "activationEvents": ["onLanguage:lua"],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "lua",
        "aliases": ["AviUtl2 Lua", "lua"],
        "extensions": [".anm2", ".obj2", ".cam2", ".scn2", ".tra2"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "lua",
        "scopeName": "source.lua",
        "path": "./syntaxes/aul2.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "lua",
        "path": "./snippets/aul2.code-snippets"
      }
    ]
  }
}